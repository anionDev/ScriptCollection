[alias]
    listfiles = ls-tree --name-only
    removelastcommit = reset HEAD~ --soft
    removeuntrackedfiles = clean -fdx
    showbranchname = rev-parse --abbrev-ref HEAD
    showfull = show --format=fuller
    showglobalconfig = config --list --show-origin
    showtrackedignored = ls-files -i --exclude-standard
